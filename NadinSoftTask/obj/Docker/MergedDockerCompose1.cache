name: dockercompose2773091460784807389
services:
  mssql:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Your_password123
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: volume
        source: mssql_data
        target: /var/opt/mssql
        volume: {}
  webapi:
    build:
      context: C:\Users\soroush\Desktop\Nadin Soft Task\NadinSoftTask
      dockerfile: WebApi/Dockerfile
    depends_on:
      mssql:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_URLS: http://+:80
      ConnectionStrings__DefaultConnection: Server=mssql;Database=NadinSoftTask;Integrated Security=true;MultipleActiveResultSets=true;TrustServerCertificate=true;
    image: webapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8080"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\soroush\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\soroush\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose2773091460784807389_default
volumes:
  mssql_data:
    name: dockercompose2773091460784807389_mssql_data